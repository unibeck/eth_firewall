		"ava": "^5.0.1",
		"esbuild": "^0.15.12",
		"eslint": "^8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-typescript": "^3.0.0",
		"miniflare": "^2.6.0",
		"prettier": "^2.7.1",
