{
	"compilerOptions": {
		"noEmit": true,
		"target": "es2020",
		"lib": ["es2020"],
		"module": "esnext",
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true,
		"alwaysStrict": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"allowOverwrite": true,
		"types": [
			"@cloudflare/workers-types",
			"@types/jest",
			"@types/uuid"
		]
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist", "test"]
}
